  "item": [
    {
      "name": "Create Board",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "let jsonData = pm.response.json();\r",
              "pm.environment.set(\"boardID\", jsonData.id );\r",
              "\r",
              "\r",
              "//Check if id, name and url exist in the response\r",
              "pm.test('Check if id exists', function() {\r",
              "    pm.expect(jsonData.id).to.exist;\r",
              "});\r",
              "\r",
              "pm.test('Check if name exists', function() {\r",
              "    pm.expect(jsonData.name).to.exist;\r",
              "});\r",
              "\r",
              "pm.test('Check if url exists', function() {\r",
              "    pm.expect(jsonData.url).to.exist;\r",
              "});\r",
              "\r",
              "let createdboardname = pm.environment.get(\"myboardName\");\r",
              "pm.test(\"Correct Board Name\", function () {\r",
              "    pm.expect(jsonData.name).to.eql(createdboardname);\r",
              "});\r",
              "\r",
              "\r",
              "pm.test(\"Status code is 200\", function () {\r",
              "    pm.response.to.have.status(200);\r",
              "});"
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        },
        {
          "listen": "prerequest",
          "script": {
            "exec": [
              "// Generate a random 3-digit number\r",
              "let randomNum = Math.floor(Math.random() * 1000).toString().padStart(3, '0');\r",
              "\r",
              "// Create the dynamic value\r",
              "let uniqueValue = `myboard${randomNum}`;\r",
              "\r",
              "// Set it as an environment or global variable\r",
              "pm.variables.set(\"myboardname\", uniqueValue); \r",
              ""
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        }
      ],
      "request": {
        "method": "POST",
        "header": [],
        "url": {
          "raw": "{{base_url}}/1/boards/?name={{myboardname}}&key={{key}}&token={{token}}",
